			"event_id": "dialogic_010",
			"options": [

			],
			"portrait": "",
			"question": "What do you think of her? I mean, you work with her, you must have an opinion."
		},
		{
			"choice": "I am Becky.",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
