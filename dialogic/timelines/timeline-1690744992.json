		{
			"character": "",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Uhh, okay. I guess you’re as weird as Becky said you were..."
		},
		{
			"choice": "I hate Becky.",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
